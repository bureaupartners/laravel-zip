{
    "name": "bureaupartners/laravel-zip",
    "description": "laravel-zip is the world's leading zip utility for file compression and backup.",
    "license": "MIT",
    "homepage": "http://www.zanysoft.co",
    "authors": [
        {
            "name": "Zany Soft",
            "email": "info@zanysoft.co",
            "homepage": "http://www.zanysoft.co"
        },
        {
            "name": "Mark Hameetman",
            "email": "mark@bureau.partners",
            "homepage": "https://bureau.partners"
        }
    ],
    "keywords": [
        "laravel-zip",
        "laravel",
        "laravel5",
        "zip",
        "unzip",
        "extract",
        "backup",
        "ZipArchive",
        "multiple",
        "merge"
    ],
    "extra": {
        "laravel": {
            "providers": [
                "BureauPartners\\Zip\\ZipServiceProvider"
            ],
            "aliases": {
                "Zip": "BureauPartners\\Zip\\ZipFacade"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "BureauPartners\\Zip\\": "src"
        }
    },
    "require": {
        "php": ">=5.6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0|^5.0",
        "scrutinizer/ocular": "^1.0"
    }
}